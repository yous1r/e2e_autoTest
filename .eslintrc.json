{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:playwright/playwright-test",
    // "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unexpected-multiline": "off"
  }
}
